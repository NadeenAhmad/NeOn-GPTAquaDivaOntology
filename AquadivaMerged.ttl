@prefix : <http://example.org/aquadiva#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://example.org/aquadiva#> .

<http://example.org/aquadiva4> rdf:type owl:Ontology ;
                                owl:imports <http://example.org/aquadiva> ;
                                dct:description "An ontology for integrating and annotating diverse datasets in groundwater ecosystems research, covering hydrogeology, microbial ecology, geochemistry, and karst systems."@en ;
                                dct:title "AquaDiva Ontology" ;
                                owl:versionInfo "1.0" .

#################################################################
#    Object Properties
#################################################################

###  http://example.org/aquadiva#hasOrganicMatterContent
:hasOrganicMatterContent rdfs:domain :SoilOrganicMatter ;
                         rdfs:range xsd:percentage .


###  http://example.org/aquadiva#hasSoilMoistureContent
:hasSoilMoistureContent rdfs:domain :SoilChemistry ;
                        rdfs:range xsd:percentage .


###  http://example.org/aquadiva#interactWith
:interactWith rdf:type owl:ObjectProperty ,
                       owl:SymmetricProperty ;
              rdfs:label "interact with" .


###  http://example.org/aquadiva#interactsWith
:interactsWith rdf:type owl:ReflexiveProperty ;
               rdfs:label "interacts with" .


###  http://example.org/aquadiva#isAffectedBy
:isAffectedBy rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:label "is affected by" .


###  http://example.org/aquadiva#isAssociatedWith
:isAssociatedWith rdf:type owl:ObjectProperty ,
                           owl:SymmetricProperty ,
                           owl:TransitiveProperty ;
                  rdfs:label "is associated with" .


###  http://example.org/aquadiva#isRelatedTo
:isRelatedTo rdf:type owl:ObjectProperty ,
                      owl:SymmetricProperty ,
                      owl:ReflexiveProperty ;
             rdfs:label "is related to" .


###  http://example.org/aquadiva#occursIn
:occursIn rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          rdfs:label "occurs in" .


#################################################################
#    Data properties
#################################################################

###  http://example.org/aquadiva#hasAnnualRainfall
:hasAnnualRainfall rdf:type owl:DatatypeProperty ;
                   rdfs:domain :GroundwaterPrecipitation ;
                   rdfs:range xsd:float ;
                   rdfs:label "has annual rainfall" .


###  http://example.org/aquadiva#hasAquiferDepth
:hasAquiferDepth rdf:type owl:DatatypeProperty ;
                 rdfs:domain :AquiferSystem ;
                 rdfs:range xsd:float ;
                 rdfs:label "has aquifer depth" .


###  http://example.org/aquadiva#hasBiodiversityIndex
:hasBiodiversityIndex rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Biodiversity ;
                      rdfs:range xsd:float ;
                      rdfs:label "has biodiversity index" .


###  http://example.org/aquadiva#hasCO2EmissionRate
:hasCO2EmissionRate rdf:type owl:DatatypeProperty ;
                    rdfs:domain :GroundwaterEcosystem ;
                    rdfs:range xsd:float ;
                    rdfs:label "has CO2 emission rate" .


###  http://example.org/aquadiva#hasChemicalConcentration
:hasChemicalConcentration rdf:type owl:DatatypeProperty ;
                          rdfs:domain :Groundwater ;
                          rdfs:range xsd:float ;
                          rdfs:label "has chemical concentration" .


###  http://example.org/aquadiva#hasInfiltrationRate
:hasInfiltrationRate rdf:type owl:DatatypeProperty ;
                     rdfs:domain :InfiltrationArea ;
                     rdfs:range xsd:float ;
                     rdfs:label "has infiltration rate" .


###  http://example.org/aquadiva#hasManagementEffectivenessScore
:hasManagementEffectivenessScore rdf:type owl:DatatypeProperty ;
                                 rdfs:domain :ManagementPractice ;
                                 rdfs:range xsd:float ;
                                 rdfs:label "has management effectiveness score" .


###  http://example.org/aquadiva#hasMicrobialDensity
:hasMicrobialDensity rdf:type owl:DatatypeProperty ;
                     rdfs:domain :Microbes ;
                     rdfs:range xsd:positiveInteger ;
                     rdfs:label "has microbial density" .


###  http://example.org/aquadiva#hasMineralDensity
:hasMineralDensity rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Mineral ;
                   rdfs:range xsd:float ;
                   rdfs:label "has mineral density" .


###  http://example.org/aquadiva#hasNitrogenLevel
:hasNitrogenLevel rdf:type owl:DatatypeProperty ;
                  rdfs:domain :NitrogenCycle ;
                  rdfs:range xsd:float ;
                  rdfs:label "has nitrogen level" .


###  http://example.org/aquadiva#hasOrganicMatterContent
:hasOrganicMatterContent rdf:type owl:DatatypeProperty .


###  http://example.org/aquadiva#hasPHLevel
:hasPHLevel rdf:type owl:DatatypeProperty ;
            rdfs:domain :GroundwaterChemistry ;
            rdfs:range xsd:float ;
            rdfs:label "has pH level" .


###  http://example.org/aquadiva#hasPermeabilityIndex
:hasPermeabilityIndex rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Permeability ;
                      rdfs:range xsd:float ;
                      rdfs:label "has permeability index" .


###  http://example.org/aquadiva#hasPollutionLevel
:hasPollutionLevel rdf:type owl:DatatypeProperty ;
                   rdfs:domain :GroundwaterContamination ;
                   rdfs:range xsd:float ;
                   rdfs:label "has pollution level" .


###  http://example.org/aquadiva#hasRockPorosity
:hasRockPorosity rdf:type owl:DatatypeProperty ;
                 rdfs:domain :RockType ;
                 rdfs:range xsd:decimal ;
                 rdfs:label "has rock porosity" .


###  http://example.org/aquadiva#hasSoilMoistureContent
:hasSoilMoistureContent rdf:type owl:DatatypeProperty .


###  http://example.org/aquadiva#hasSpeciesCount
:hasSpeciesCount rdf:type owl:DatatypeProperty ;
                 rdfs:domain :RareSpeciesBiodiversity ;
                 rdfs:range xsd:positiveInteger ;
                 rdfs:label "has species count" .


###  http://example.org/aquadiva#hasTemperature
:hasTemperature rdf:type owl:DatatypeProperty ;
                rdfs:domain :Groundwater ;
                rdfs:range xsd:decimal ;
                rdfs:label "has temperature" .


###  http://example.org/aquadiva#hasTraceGasConcentration
:hasTraceGasConcentration rdf:type owl:DatatypeProperty ;
                          rdfs:domain :TraceGas ;
                          rdfs:range xsd:float ;
                          rdfs:label "has trace gas concentration" .


###  http://example.org/aquadiva#hasVegetationDensity
:hasVegetationDensity rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Vegetation ;
                      rdfs:range xsd:positiveInteger ;
                      rdfs:label "has vegetation density" .


###  http://example.org/aquadiva#hasWaterFlowSpeed
:hasWaterFlowSpeed rdf:type owl:DatatypeProperty ;
                   rdfs:domain :WaterFlow ;
                   rdfs:range xsd:float ;
                   rdfs:label "has water flow speed" .


#################################################################
#    Classes
#################################################################

###  http://example.org/aquadiva#AbioticFactor
:AbioticFactor rdf:type owl:Class ;
               owl:equivalentClass :NonLivingFactor ;
               rdfs:subClassOf :EcologicalFactor ;
               owl:disjointWith :BioticFactor ;
               rdfs:comment "Non-living chemical and physical parts of the environment that affect living organisms and the functioning of ecosystems." ;
               rdfs:label "Abiotic Factor" .


###  http://example.org/aquadiva#AgriculturalActivity
:AgriculturalActivity rdf:type owl:Class ;
                      owl:equivalentClass :Agriculture ;
                      rdfs:subClassOf :LandUseActivity ;
                      owl:disjointWith :UrbanActivity ;
                      rdfs:comment "Farming practices and operations that can impact environmental quality, including groundwater systems, through the use of fertilizers, pesticides, and water management." ;
                      rdfs:label "Agricultural Activity" .


###  http://example.org/aquadiva#AgriculturalArea
:AgriculturalArea owl:equivalentClass :IndustrialAgricultureArea .


###  http://example.org/aquadiva#AgriculturalPractice
:AgriculturalPractice rdf:type owl:Class ;
                      owl:equivalentClass :FarmingPractice ;
                      rdfs:subClassOf :LandUseActivity ;
                      owl:disjointWith :NonAgriculturalPractice ;
                      rdfs:comment "Methods and techniques employed in cultivation and livestock rearing, which impact environmental quality and productivity." ;
                      rdfs:label "Agricultural Practice" .


###  http://example.org/aquadiva#AmazonRainforestSoil
:AmazonRainforestSoil rdf:type owl:Class ;
                      owl:equivalentClass :RainforestSoil ;
                      rdfs:subClassOf :Soil ;
                      owl:disjointWith :NonRainforestSoil .


###  http://example.org/aquadiva#AnalysisMethod
:AnalysisMethod rdf:type owl:Class ;
                owl:equivalentClass :SampleAnalysisMethod ;
                rdfs:subClassOf :Method ;
                owl:disjointWith :NonAnalysisMethod ;
                rdfs:comment "Techniques used to examine samples in order to discover their composition, structure, or properties, often used in scientific research." ;
                rdfs:label "Analysis Method" .


###  http://example.org/aquadiva#AquaticFungi
:AquaticFungi rdf:type owl:Class ;
              owl:equivalentClass :AquaticMicroorganism ,
                                  :Fungi ;
              rdfs:subClassOf :Fungi ;
              owl:disjointWith :TerrestrialFungi ;
              rdfs:label "Aquatic Fungi" .


###  http://example.org/aquadiva#AquaticMicrobes
:AquaticMicrobes rdf:type owl:Class ;
                 owl:disjointWith :TerrestrialMicrobes ;
                 rdfs:label "Aquatic Microbes" .


###  http://example.org/aquadiva#AquaticMicrobialCommunity
:AquaticMicrobialCommunity rdf:type owl:Class ;
                           owl:equivalentClass :MicrobialCommunity ;
                           rdfs:subClassOf :Community ;
                           owl:disjointWith :TerrestrialMicrobialCommunity .


###  http://example.org/aquadiva#AquaticSystem
:AquaticSystem owl:disjointWith :AquiferSystem .


###  http://example.org/aquadiva#AquiferChemistry
:AquiferChemistry rdf:type owl:Class ;
                  owl:equivalentClass :Hydrochemistry ;
                  rdfs:subClassOf :Geochemistry ;
                  owl:disjointWith :SoilChemistry ;
                  rdfs:comment "Chemical characteristics and processes occurring within an aquifer, including solute transport and reactions." ;
                  rdfs:label "Aquifer Chemistry" .


###  http://example.org/aquadiva#AquiferComparison
:AquiferComparison owl:equivalentClass :GroundwaterComparison .


###  http://example.org/aquadiva#AquiferEcosystem
:AquiferEcosystem owl:equivalentClass :GroundwaterEcosystem .


###  http://example.org/aquadiva#AquiferFungiSample
:AquiferFungiSample rdf:type owl:Class ;
                    owl:equivalentClass :FungiSample ;
                    rdfs:subClassOf :Fungi ;
                    owl:disjointWith :NonFungiSample .


###  http://example.org/aquadiva#AquiferProtection
:AquiferProtection owl:equivalentClass :GroundwaterProtection .


###  http://example.org/aquadiva#AquiferRechargeZone
:AquiferRechargeZone owl:equivalentClass :RechargeZone .


###  http://example.org/aquadiva#AquiferSystem
:AquiferSystem rdf:type owl:Class ;
               owl:equivalentClass :ChalkAquifer ,
                                   :GroundwaterSystem ;
               rdfs:subClassOf :HydrogeologicalSystem ;
               rdfs:comment "An underground layer of water-bearing permeable rock or materials such as gravel, sand, or silt." ;
               rdfs:label "Aquifer System" .


###  http://example.org/aquadiva#AquiferWater
:AquiferWater owl:equivalentClass :Groundwater .


###  http://example.org/aquadiva#ArtificialStrata
:ArtificialStrata owl:disjointWith :GeologicalStrata .


###  http://example.org/aquadiva#ArtificialSurface
:ArtificialSurface owl:disjointWith :Vegetation .


###  http://example.org/aquadiva#AtmosphericTraceGas
:AtmosphericTraceGas owl:equivalentClass :TraceGas .


###  http://example.org/aquadiva#Bacteria
:Bacteria owl:disjointWith :Fungi .


###  http://example.org/aquadiva#BacterialCulture
:BacterialCulture owl:equivalentClass :SubsurfaceBacterialCulture .


###  http://example.org/aquadiva#BareGround
:BareGround owl:disjointWith :VegetationCover .


###  http://example.org/aquadiva#Biodiversity
:Biodiversity rdf:type owl:Class ;
              owl:equivalentClass :EcologicalDiversity ;
              rdfs:subClassOf :EcologicalMetric ;
              owl:disjointWith :Monoculture ;
              rdfs:comment "The variety and variability of life forms within a given ecosystem, region, or on the entire Earth, important in assessing the health and function of ecosystems." ;
              rdfs:label "Biodiversity" .


###  http://example.org/aquadiva#BiogeochemicalCycle
:BiogeochemicalCycle owl:equivalentClass :NitrogenCycle .


###  http://example.org/aquadiva#BiologicalCompound
:BiologicalCompound owl:disjointWith :ChemicalCompound .


###  http://example.org/aquadiva#Biomarker
:Biomarker owl:equivalentClass :MolecularMarker .


###  http://example.org/aquadiva#BioticFactor
:BioticFactor rdf:type owl:Class ;
              owl:equivalentClass :LivingFactor ;
              rdfs:subClassOf :EcologicalFactor ;
              rdfs:comment "A living element of an ecosystem that influences or interacts with other organisms and the environment, such as plants, animals, and microorganisms." ;
              rdfs:label "Biotic Factor" .


###  http://example.org/aquadiva#Bioturbation
:Bioturbation rdf:type owl:Class ;
              owl:equivalentClass :SoilDisturbance ;
              rdfs:subClassOf :EcologicalProcess ;
              owl:disjointWith :Sedimentation ;
              rdfs:comment "The disturbance of sedimentary deposits by living organisms, often affecting soil composition and structure." ;
              rdfs:label "Bioturbation" .


###  http://example.org/aquadiva#CO2EmissionPathway
:CO2EmissionPathway owl:disjointWith :CO2FixationPathway .


###  http://example.org/aquadiva#CO2FixationPathway
:CO2FixationPathway rdf:type owl:Class ;
                    owl:equivalentClass :CarbonFixationPathway ;
                    rdfs:subClassOf :BiochemicalPathway ;
                    rdfs:comment "Biochemical routes by which CO2 is converted into organic molecules by living organisms, crucial for carbon cycling and storage." ;
                    rdfs:label "CO2 Fixation Pathway" .


###  http://example.org/aquadiva#CO2ReductionPathway
:CO2ReductionPathway rdf:type owl:Class ;
                     owl:equivalentClass :CarbonFixationPathway ;
                     rdfs:subClassOf :BiochemicalPathway ;
                     owl:disjointWith :NonCarbonFixationPathway .


###  http://example.org/aquadiva#CarbonCycle
:CarbonCycle owl:disjointWith :NitrogenCycle .


###  http://example.org/aquadiva#CarbonateDissolution
:CarbonateDissolution rdf:type owl:Class ;
                      owl:equivalentClass :CarbonateDissortation ;
                      rdfs:subClassOf :ChemicalProcess ;
                      owl:disjointWith :SilicateDissolution ;
                      rdfs:comment "A process where carbonate minerals (e.g., calcite, dolomite) dissolve in water, significantly affecting karst aquifer systems." ;
                      rdfs:label "Carbonate Dissolution" .


###  http://example.org/aquadiva#CarbonateDissortation
:CarbonateDissortation rdf:type owl:Class ;
                       owl:equivalentClass :CarbonateProcess ;
                       rdfs:subClassOf :ChemicalProcess ;
                       owl:disjointWith :NonCarbonateProcess ;
                       rdfs:label "Carbonate Dissortation" .


###  http://example.org/aquadiva#CatchmentArea
:CatchmentArea rdf:type owl:Class ;
               owl:equivalentClass :HydrologicalCatchment ;
               rdfs:subClassOf :HydrologicalUnit ;
               owl:disjointWith :NonCatchment ,
                                :NonCatchmentArea ;
               rdfs:comment "The area from which rainfall and surface water flow into a particular river, lake, or reservoir." ;
               rdfs:label "Catchment Area" .


###  http://example.org/aquadiva#ChalkAquifer
:ChalkAquifer rdf:type owl:Class ;
              owl:equivalentClass :LimestoneAquifer ;
              rdfs:subClassOf :AquiferSystem ;
              owl:disjointWith :NonAquiferSystem .


###  http://example.org/aquadiva#ChemicalCompound
:ChemicalCompound rdf:type owl:Class ;
                  rdfs:subClassOf :GroundwaterChemical ;
                  rdfs:comment "A substance consisting of two or more different elements chemically bonded together, found in groundwater." ;
                  rdfs:label "Chemical Compound" .


###  http://example.org/aquadiva#ChemicalData
:ChemicalData owl:equivalentClass :MolecularFormula .


###  http://example.org/aquadiva#ChemicalMarker
:ChemicalMarker owl:equivalentClass :Miz .


###  http://example.org/aquadiva#ChronologicalPattern
:ChronologicalPattern owl:equivalentClass :TemporalPattern .


###  http://example.org/aquadiva#CleanSite
:CleanSite owl:disjointWith :ContaminatedSite .


###  http://example.org/aquadiva#CommonSpeciesBiodiversity
:CommonSpeciesBiodiversity owl:disjointWith :RareSpeciesBiodiversity .


###  http://example.org/aquadiva#CommonSpeciesDiversity
:CommonSpeciesDiversity owl:disjointWith :RareSpeciesBiodiversity .


###  http://example.org/aquadiva#ComplexAquiferSystem
:ComplexAquiferSystem owl:equivalentClass :MultilevelGroundwaterSystem .


###  http://example.org/aquadiva#ConductivityStudy
:ConductivityStudy owl:equivalentClass :HydraulicConductivityStudy .


###  http://example.org/aquadiva#ConstantCondition
:ConstantCondition owl:disjointWith :SeasonalVariation .


###  http://example.org/aquadiva#Consumption
:Consumption rdf:type owl:Class ;
             owl:equivalentClass :EcologicalConsumption ;
             rdfs:subClassOf :EcologicalProcess ;
             owl:disjointWith :Production ;
             rdfs:comment "The use or depletion of resources or energy, commonly in biological contexts such as metabolism or ecological interactions." ;
             rdfs:label "Consumption" .


###  http://example.org/aquadiva#ContaminatedSite
:ContaminatedSite rdf:type owl:Class ;
                  owl:equivalentClass :PollutedSite ;
                  rdfs:subClassOf :Site .


###  http://example.org/aquadiva#Contamination
:Contamination owl:equivalentClass :GroundwaterContamination .


###  http://example.org/aquadiva#CriticalZone
:CriticalZone owl:equivalentClass :HainichCriticalZone .


###  http://example.org/aquadiva#CriticalZoneObservatory
:CriticalZoneObservatory owl:equivalentClass :HainichCZObservatory .


###  http://example.org/aquadiva#DOM
:DOM rdf:type owl:Class ;
     owl:equivalentClass :DissolvedOrganicMatter ;
     rdfs:subClassOf :OrganicMatter ;
     owl:disjointWith :POM ;
     rdfs:comment "Dissolved organic matter in water, which includes a complex mixture of organic compounds that affects water chemistry and microbial activity." ;
     rdfs:label "DOM" .


###  http://example.org/aquadiva#Decomposition
:Decomposition owl:disjointWith :Production .


###  http://example.org/aquadiva#DeepAquifer
:DeepAquifer rdf:type owl:Class ;
             owl:equivalentClass :SubsurfaceAquifer ;
             rdfs:subClassOf :AquiferSystem ;
             owl:disjointWith :ShallowAquifer .


###  http://example.org/aquadiva#DesertEcosystem
:DesertEcosystem owl:disjointWith :ForestEcosystem .


###  http://example.org/aquadiva#DesertSpring
:DesertSpring rdf:type owl:Class ;
              owl:equivalentClass :Spring ;
              rdfs:subClassOf :WaterSource ;
              owl:disjointWith :NonSpring .


###  http://example.org/aquadiva#DrySoil
:DrySoil owl:disjointWith :SoilWaterContent .


###  http://example.org/aquadiva#EcologicalProduction
:EcologicalProduction owl:equivalentClass :Production .


###  http://example.org/aquadiva#EcologicalSignal
:EcologicalSignal owl:equivalentClass :EnvironmentalSignal .


###  http://example.org/aquadiva#EmpiricalFormula
:EmpiricalFormula owl:disjointWith :MolecularFormula .


###  http://example.org/aquadiva#EnergyMetabolismPathway
:EnergyMetabolismPathway rdf:type owl:Class ;
                         owl:equivalentClass :MetabolicPathway ;
                         rdfs:subClassOf :BiochemicalPathway ;
                         owl:disjointWith :NonMetabolicPathway ;
                         rdfs:comment "Biochemical routes by which organisms transform energy from one form to another, crucial for cellular processes and ecological dynamics." ;
                         rdfs:label "Energy Metabolism Pathway" .


###  http://example.org/aquadiva#EnvironmentalManagement
:EnvironmentalManagement owl:equivalentClass :ManagementPractice .


###  http://example.org/aquadiva#EnvironmentalSignal
:EnvironmentalSignal rdf:type owl:Class ;
                     rdfs:subClassOf :Signal ;
                     owl:disjointWith :Noise ;
                     rdfs:comment "Natural or anthropogenic indicators that provide information about environmental conditions or changes, used for monitoring and management purposes." ;
                     rdfs:label "Environmental Signal" .


###  http://example.org/aquadiva#EnvironmentalVulnerability
:EnvironmentalVulnerability owl:equivalentClass :GroundwaterVulnerability .


###  http://example.org/aquadiva#FixedOrganicMatter
:FixedOrganicMatter owl:disjointWith :MobileOrganicMatter .


###  http://example.org/aquadiva#FlatSurface
:FlatSurface owl:disjointWith :Terrain .


###  http://example.org/aquadiva#ForestEcosystem
:ForestEcosystem rdf:type owl:Class ;
                 owl:equivalentClass :ForestEnvironment ;
                 rdfs:subClassOf :Ecosystem ;
                 rdfs:comment "An ecological system within a forest setting, encompassing the interactions among plants, animals, microorganisms, and their environment." ;
                 rdfs:label "Forest Ecosystem" .


###  http://example.org/aquadiva#ForestHabitat
:ForestHabitat owl:equivalentClass :ForestMicrohabitat .


###  http://example.org/aquadiva#ForestMicrohabitat
:ForestMicrohabitat rdf:type owl:Class ;
                    rdfs:subClassOf :Microhabitat ;
                    owl:disjointWith :NonForestHabitat .


###  http://example.org/aquadiva#FracturedAquifer
:FracturedAquifer owl:equivalentClass :FracturedRockAquifer .


###  http://example.org/aquadiva#FracturedRock
:FracturedRock rdf:type owl:Class ;
               owl:equivalentClass :RockFracture ;
               rdfs:subClassOf :GeologicalStrata ;
               owl:disjointWith :SolidRock ;
               rdfs:comment "Rock formations that have significant fracture systems, influencing groundwater flow and storage." ;
               rdfs:label "Fractured Rock" .


###  http://example.org/aquadiva#FracturedRockAquifer
:FracturedRockAquifer rdf:type owl:Class ;
                      rdfs:subClassOf :Aquifer ;
                      owl:disjointWith :KarstAquifer ,
                                       :SolidRockAquifer .


###  http://example.org/aquadiva#Fungi
:Fungi rdf:type owl:Class ;
       rdfs:subClassOf :Microorganism ;
       owl:disjointWith :Microbes ;
       rdfs:label "Fungi" .


###  http://example.org/aquadiva#GeochemicalCharacterizationMethod
:GeochemicalCharacterizationMethod owl:equivalentClass :HydrogeochemicalCharacterizationMethod .


###  http://example.org/aquadiva#Geochemistry
:Geochemistry rdf:type owl:Class ;
              owl:equivalentClass :GeologicalChemistry ;
              rdfs:subClassOf :EarthScience ;
              owl:disjointWith :NonGeochemistry ;
              rdfs:label "Geochemistry" .


###  http://example.org/aquadiva#GeologicalMaterial
:GeologicalMaterial owl:equivalentClass :RockType .


###  http://example.org/aquadiva#GeologicalMineral
:GeologicalMineral owl:equivalentClass :Mineral .


###  http://example.org/aquadiva#GeologicalStrata
:GeologicalStrata rdf:type owl:Class ;
                  owl:equivalentClass :Strata ;
                  rdfs:subClassOf :Strata ;
                  rdfs:comment "Layers of sedimentary rock or soil with consistent characteristics that can influence groundwater movement and storage." ;
                  rdfs:label "Geological Strata" .


###  http://example.org/aquadiva#GeospatialPattern
:GeospatialPattern owl:equivalentClass :SpatialPattern .


###  http://example.org/aquadiva#Groundwater
:Groundwater rdf:type owl:Class ;
             owl:equivalentClass :HighCalciumWater ;
             rdfs:subClassOf :Water ;
             owl:disjointWith :SurfaceWater ;
             rdfs:comment "Water located beneath the earth's surface in soil pore spaces and in the fractures of rock formations." ;
             rdfs:label "Groundwater" .


###  http://example.org/aquadiva#GroundwaterChemistry
:GroundwaterChemistry rdf:type owl:Class ;
                      owl:equivalentClass :Hydrochemistry ;
                      rdfs:subClassOf :Geochemistry ;
                      owl:disjointWith :SurfaceWaterChemistry ;
                      rdfs:comment "The chemical composition and properties of groundwater, including solutes and gases, which affect its quality and ecological role." ;
                      rdfs:label "Groundwater Chemistry" .


###  http://example.org/aquadiva#GroundwaterComparison
:GroundwaterComparison rdf:type owl:Class ;
                       rdfs:subClassOf :GroundwaterStudy ;
                       owl:disjointWith :SurfaceWaterComparison ;
                       rdfs:label "Groundwater Comparison" .


###  http://example.org/aquadiva#GroundwaterContamination
:GroundwaterContamination rdf:type owl:Class ;
                          rdfs:subClassOf :Contamination ;
                          owl:disjointWith :GroundwaterPurity ;
                          rdfs:comment "The presence of harmful substances in groundwater, which can compromise water quality and ecosystem health." ;
                          rdfs:label "Groundwater Contamination" .


###  http://example.org/aquadiva#GroundwaterEcosystem
:GroundwaterEcosystem rdf:type owl:Class ;
                      rdfs:subClassOf :Ecosystem ;
                      owl:disjointWith :SurfaceWaterEcosystem ;
                      rdfs:comment "An ecological system located below the earth's surface, encompassing interactions among water, microorganisms, chemicals, and geological materials." ;
                      rdfs:label "Groundwater Ecosystem" .


###  http://example.org/aquadiva#GroundwaterFlow
:GroundwaterFlow rdf:type owl:Class ;
                 owl:equivalentClass :SubsurfaceWaterFlow ;
                 rdfs:subClassOf :HydrologicalProcess ;
                 owl:disjointWith :SurfaceWaterFlow ;
                 rdfs:comment "The movement of water through aquifer systems, driven by gradients in pressure and elevation, crucial for water supply and ecosystem support." ;
                 rdfs:label "Groundwater Flow" .


###  http://example.org/aquadiva#GroundwaterPhysiochemicalParameters
:GroundwaterPhysiochemicalParameters rdf:type owl:Class ;
