# Hierarchical Structure for CO2 Fixation Pathway
:CO2FixationPathway rdf:type owl:Class ;
    rdfs:subClassOf :FixationPathway ;
    rdfs:comment "Pathways involved in the fixation of CO2 into organic molecules.".

:CalvinCycle rdf:type owl:Class ;
    rdfs:subClassOf :CO2FixationPathway ;
    rdfs:comment "A CO2 fixation pathway where CO2 is converted into glucose via the Calvin-Benson cycle.".

:C4Pathway rdf:type owl:Class ;
    rdfs:subClassOf :CO2FixationPathway ;
    rdfs:comment "A CO2 fixation pathway found in some plants where CO2 is first fixed into a four-carbon compound.".

:CAMPathway rdf:type owl:Class ;
    rdfs:subClassOf :CO2FixationPathway ;
    rdfs:comment "A CO2 fixation pathway found in some plants where CO2 is fixed at night and stored as a four-carbon acid.".

# Hierarchical Structure for Nitrogen Cycle
:NitrogenCycle rdf:type owl:Class ;
    rdfs:subClassOf :BiogeochemicalCycle ;
    rdfs:comment "The biogeochemical cycle that describes the transformations of nitrogen and nitrogen-containing compounds in nature.".

:NitrogenFixation rdf:type owl:Class ;
    rdfs:subClassOf :NitrogenCycle ;
    rdfs:comment "The process by which nitrogen gas from the atmosphere is converted into ammonia by microorganisms.".

:Nitrification rdf:type owl:Class ;
    rdfs:subClassOf :NitrogenCycle ;
    rdfs:comment "The process by which ammonia is converted to nitrate ions (NO3).".

:AmmoniaOxidation rdf:type owl:Class ;
    rdfs:subClassOf :Nitrification ;
    rdfs:comment "The first step in nitrification where ammonia is oxidized to nitrite.".

:NitriteOxidation rdf:type owl:Class ;
    rdfs:subClassOf :Nitrification ;
    rdfs:comment "The second step in nitrification where nitrite is oxidized to nitrate.".

:Denitrification rdf:type owl:Class ;
    rdfs:subClassOf :NitrogenCycle ;
    rdfs:comment "The microbial process of reducing nitrate and nitrite to gaseous forms of nitrogen, such as N2O and N2.".

:Anammox rdf:type owl:Class ;
    rdfs:subClassOf :NitrogenCycle ;
    rdfs:comment "The anaerobic oxidation of ammonia to nitrogen gas.".

# Hierarchical Structure for Forest Ecosystem
:ForestEcosystem rdf:type owl:Class ;
    rdfs:subClassOf :Ecosystem ;
    rdfs:comment "Ecosystems that are dominated by trees and other vegetation.".

:BeechForest rdf:type owl:Class ;
    rdfs:subClassOf :ForestEcosystem ;
    rdfs:comment "Forests predominantly composed of beech trees.".

:TropicalForest rdf:type owl:Class ;
    rdfs:subClassOf :ForestEcosystem ;
    rdfs:comment "Forests found in tropical regions with high biodiversity and complex structure.".

:TemperateForest rdf:type owl:Class ;
    rdfs:subClassOf :ForestEcosystem ;
    rdfs:comment "Forests found in temperate regions, typically with distinct seasons.".

# Hierarchical Structure for Trace Gases
:TraceGas rdf:type owl:Class ;
    rdfs:subClassOf :Gas ;
    rdfs:comment "Gases that are present in the atmosphere at low concentrations.".

:Methane rdf:type owl:Class ;
    rdfs:subClassOf :TraceGas ;
    rdfs:comment "A trace gas with the chemical formula CH4, produced by methanogenic bacteria.".

:NitrousOxide rdf:type owl:Class ;
    rdfs:subClassOf :TraceGas ;
    rdfs:comment "A trace gas with the chemical formula N2O, produced by microbial processes in soils and water bodies.".

:CarbonDioxide rdf:type owl:Class ;
    rdfs:subClassOf :TraceGas ;
    rdfs:comment "A trace gas with the chemical formula CO2, produced by respiration and combustion.".

# Hierarchical Structure for Microorganisms
:Microorganism rdf:type owl:Class ;
    rdfs:subClassOf :Organism ;
    rdfs:comment "Microscopic organisms, such as bacteria, archaea, and fungi.".

:AmmoniaOxidizer rdf:type owl:Class ;
    rdfs:subClassOf :Microorganism ;
    rdfs:comment "Microorganisms that convert ammonia to nitrite in the nitrogen cycle.".

:NitriteOxidizer rdf:type owl:Class ;
    rdfs:subClassOf :Microorganism ;
    rdfs:comment "Microorganisms that convert nitrite to nitrate in the nitrogen cycle.".

:DenitrifyingBacteria rdf:type owl:Class ;
    rdfs:subClassOf :Microorganism ;
    rdfs:comment "Bacteria that convert nitrate to nitrogen gas in the denitrification process.".

:Methanogens rdf:type owl:Class ;
    rdfs:subClassOf :Microorganism ;
    rdfs:comment "Archaea that produce methane as a metabolic byproduct in anoxic conditions.".

