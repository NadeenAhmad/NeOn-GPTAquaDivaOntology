### Object Property Definitions ###

aqua:inhabits           rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:Microorganisms ;
                        rdfs:range aqua:Aquifer .

aqua:affects            rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:Processes ;
                        rdfs:range aqua:Groundwater .

aqua:influences         rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:Processes ;
                        rdfs:range aqua:GroundwaterSystems .

aqua:associatedWith     rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:AquiferChemicalData ;
                        rdfs:range aqua:MicrobialCommunities .

aqua:isPartOf           rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:GeochemicalSignals ;
                        rdfs:range aqua:Groundwater .

aqua:contributesTo      rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:Microorganisms ;
                        rdfs:range aqua:NitrogenCycling .

aqua:transports         rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:Biocolloids ;
                        rdfs:range aqua:Microorganisms .

aqua:indicates          rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:GeochemicalSignals ;
                        rdfs:range aqua:MicrobialPopulationChanges .

aqua:variesAcross       rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:CommunityStructures ;
                        rdfs:range aqua:AquiferDepth .

aqua:comparesWith       rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:MicrobialDiversity ;
                        rdfs:range aqua:SubsurfaceAquifers .

aqua:drives             rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:KeyDrivers ;
                        rdfs:range aqua:OrganicMatterMobilization .

aqua:uses               rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:BestPractices ;
                        rdfs:range aqua:SamplingMethods .

aqua:hasRoleIn          rdf:type owl:ObjectProperty ;
                        rdfs:domain aqua:Microorganisms ;
                        rdfs:range aqua:AquiferEcosystem .

aqua:mitigates          rdf:type owl:ObjectProperty ;
