### Properties

# Object Properties
:hasDominantFunctionalGuild rdf:type owl:ObjectProperty ;
    rdfs:domain :aquifer ;
    rdfs:range :MicrobialFunctionalGuild .

:presentIn rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range :aquifer .

:contributeTo rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range :GeochemicalProcess .

:hasHabitatDistribution rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialSpecies ;
    rdfs:range :aquifer .

:inAquiferWith rdf:type owl:ObjectProperty ;
    rdfs:domain :aquifer ;
    rdfs:range :ChemicalComposition .

:comparedTo rdf:type owl:ObjectProperty ;
    rdfs:domain :MicrobialCommunity ;
    rdfs:range :AbovegroundEnvironment .

:variesBetween rdf:type owl:ObjectProperty ;
    rdfs:domain :Hydrochemistry ;
    rdfs:range [ rdf:type owl:Class ] .

:affects rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range [ rdf:type owl:Class ] .

:interactsWith rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range [ rdf:type owl:Class ] .

:foundIn rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range :SubsurfaceHabitat .

:influences rdf:type owl:ObjectProperty ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range [ rdf:type owl:Class ] .

:affectsBehaviorOf rdf:type owl:ObjectProperty ;
    rdfs:domain :CarbonateDissortation ;
    rdfs:range :EukaryoticOrganism .

:changesWith rdf:type owl:ObjectProperty ;
    rdfs:domain :PhysiochemicalParameters ;
    rdfs:range :AquiferSupply .

:playsRoleIn rdf:type owl:ObjectProperty ;
    rdfs:domain :Biocolloid ;
    rdfs:range [ rdf:type owl:Class ] .

# Other object properties are defined similarly, linking entities as described in the triples above.


### Inverse Object Properties

:isDominantFunctionalGuildOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasDominantFunctionalGuild ;
    rdfs:domain :MicrobialFunctionalGuild ;
    rdfs:range :aquifer .

:isPresentIn rdf:type owl:ObjectProperty ;
    owl:inverseOf :presentIn ;
    rdfs:domain :aquifer ;
    rdfs:range [ rdf:type owl:Class ] .

:isContributedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :contributeTo ;
    rdfs:domain :GeochemicalProcess ;
    rdfs:range [ rdf:type owl:Class ] .

:isHabitatDistributionFor rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasHabitatDistribution ;
    rdfs:domain :aquifer ;
    rdfs:range :MicrobialSpecies .

:hasAquiferWith rdf:type owl:ObjectProperty ;
    owl:inverseOf :inAquiferWith ;
    rdfs:domain :ChemicalComposition ;
    rdfs:range :aquifer .

:isComparedTo rdf:type owl:ObjectProperty ;
    owl:inverseOf :comparedTo ;
    rdfs:domain :AbovegroundEnvironment ;
    rdfs:range :MicrobialCommunity .

:variedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :variesBetween ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range :Hydrochemistry .

:isAffectedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :affects ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range [ rdf:type owl:Class ] .

:hasInteractionWith rdf:type owl:ObjectProperty ;
    owl:inverseOf :interactsWith ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range [ rdf:type owl:Class ] .

:isFoundIn rdf:type owl:ObjectProperty ;
    owl:inverseOf :foundIn ;
    rdfs:domain :SubsurfaceHabitat ;
    rdfs:range [ rdf:type owl:Class ] .

:isInfluencedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :influences ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range [ rdf:type owl:Class ] .

:isBehaviorAffectedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :affectsBehaviorOf ;
    rdfs:domain :EukaryoticOrganism ;
    rdfs:range :CarbonateDissortation .

:hasChangedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :changesWith ;
    rdfs:domain :AquiferSupply ;
    rdfs:range :PhysiochemicalParameters .

:isRoleOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :playsRoleIn ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range :Biocolloid .

:isDominatedIn rdf:type owl:ObjectProperty ;
    owl:inverseOf :dominatesIn ;
    rdfs:domain :PristineAquifer ;
    rdfs:range :BiogeochemicalProcess .

:isInstructedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :associatedWith ;
    rdfs:domain :SubsurfaceEcosystem ;
    rdfs:range :Peptide .

:isPartOf rdf:type owl:ObjectProperty ;
    owl:inverseOf :hasPart ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range [ rdf:type owl:Class ] .

:isImpactedBy rdf:type owl:ObjectProperty ;
    owl:inverseOf :impacts ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range [ rdf:type owl:Class ] .

:hasRole rdf:type owl:ObjectProperty ;
    owl:inverseOf :maintains ;
    rdfs:domain [ rdf:type owl:Class ] ;
    rdfs:range :GroundwaterQuality .

:isUsedFor rdf:type owl:ObjectProperty ;
    owl:inverseOf :usesMethod ;
    rdfs:domain :GroundwaterProtection ;
    rdfs:range :SamplingMethod .

:hasComparison rdf:type owl:ObjectProperty ;
    owl:inverseOf :comparedTo ;
    rdfs:domain :SamplingMethod ;
    rdfs:range :EnvironmentalStudy .

:isModeledAfter rdf:type owl:ObjectProperty ;
    owl:inverseOf :observedBy ;
    rdfs:domain :MicrobialBiosample ;
    rdfs:range :GroundwaterMicroorganism .


