:isInhibitedBy rdf:type owl:ObjectProperty ;
    rdfs:label "is inhibited by" ;
    rdfs:domain :BiologicalFunction ;
    rdfs:range :Inhibitor .

:contains rdf:type owl:ObjectProperty ;
    rdfs:label "contains" ;
    rdfs:domain :AquiferSystem ;
    rdfs:range :ChemicalEntity .

:hasElectronDonor rdf:type owl:ObjectProperty ;
    rdfs:label "has electron donor" ;
    rdfs:domain :ChemicalRole ;
    rdfs:range :ElectronDonor .
### Data properties
:hasConcentration rdf:type owl:DatatypeProperty ;
    rdfs:label "has concentration" ;
    rdfs:domain :ChemicalEntity ;
    rdfs:range xsd:float .

:haspH rdf:type owl:DatatypeProperty ;
    rdfs:label "has pH" ;
    rdfs:domain :EnvironmentalContainmentFactor ;
    rdfs:range xsd:float .

:hasTemperature rdf:type owl:DatatypeProperty ;
    rdfs:label "has temperature" ;
    rdfs:domain :EnvironmentalContainmentFactor ;
    rdfs:range xsd:float .

:hasContaminantLevel rdf:type owl:DatatypeProperty ;
    rdfs:label "has contaminant level" ;
    rdfs:domain :Contaminant ;
    rdfs:range xsd:float .
