aqua:OxicConditions rdf:type owl:Class ;
    rdfs:subClassOf aqua:EnvironmentalConditions ;
    rdfs:label "Oxic Conditions"@en ;
    rdfs:comment "Aquifer environments rich in dissolved oxygen."@en .

### MicrobialFunctionalGuild Hierarchy
aqua:HeterotrophicDecomposers rdf:type owl:Class ;
    rdfs:subClassOf aqua:HeterotrophicMicrobes ;
    rdfs:label "Heterotrophic Decomposers"@en ;
    rdfs:comment "Microbes that decompose organic matter by consuming carbon from external sources."@en .

aqua:AutotrophicCarbonFixers rdf:type owl:Class ;
    rdfs:subClassOf aqua:AutotrophicMicrobes ;
    rdfs:label "Autotrophic Carbon Fixers"@en ;
    rdfs:comment "Microbes that fix carbon dioxide through processes like photosynthesis or chemosynthesis."@en .
### Object Properties for Microbial Interactions
aqua:engagesIn rdf:type owl:ObjectProperty ;
    rdfs:domain aqua:MicrobialFunctionalGuild ;
    rdfs:range aqua:BiogeochemicalCycling ;
    rdfs:label "engages in"@en ;
    rdfs:comment "Relates a microbial functional guild to a biogeochemical process it participates in."@en .

aqua:inhabits rdf:type owl:ObjectProperty ;
    rdfs:domain aqua:AquiferMicrobes ;
    rdfs:range aqua:Aquifer ;
    rdfs:label "inhabits"@en ;
    rdfs:comment "Specifies the aquifer that a specific microbial community inhabits."@en .

aqua:alters rdf:type owl:ObjectProperty ;
    rdfs:domain aqua:AquiferMicrobes ;
    rdfs:range aqua:EnvironmentalConditions ;
    rdfs:label "alters"@en ;
    rdfs:comment "Specifies how microbial activity alters or influences the environmental conditions."@en .

aqua:dependsOn rdf:type owl:ObjectProperty ;
    rdfs:domain aqua:AquiferMicrobes ;
    rdfs:range aqua:EnvironmentalConditions ;
    rdfs:label "depends on"@en ;
