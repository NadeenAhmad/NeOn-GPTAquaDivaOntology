    rdfs:domain :EnvironmentalMaterial ;
    rdfs:range xsd:float ;
    rdfs:comment "The carbon content present in a material, important for understanding carbon cycling."@en ;
    rdfs:label "has_carbon_content"@en .

# hasNitrogenContent (data property: represents the nitrogen content in a material)
:hasNitrogenContent rdf:type owl:DatatypeProperty ;
    rdfs:domain :EnvironmentalMaterial ;
    rdfs:range xsd:float ;
    rdfs:comment "The nitrogen content in a material, crucial for understanding nitrogen cycling processes."@en ;
    rdfs:label "has_nitrogen_content"@en .

# hasFlowRate (data property: represents the flow rate in an aquatic system or groundwater)
:hasFlowRate rdf:type owl:DatatypeProperty ;
    rdfs:domain :GroundwaterSystem ;
    rdfs:range xsd:float ;
    rdfs:comment "The rate of flow within a groundwater system or other aquatic environments, influencing the distribution of elements and compounds."@en ;
